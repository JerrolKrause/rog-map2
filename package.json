{
  "name": "angular-starter",
  "version": "2.2.0",
  "license": "MIT",
  "scripts": {
    "localize": "git update-index --skip-worktree src/environments/environment.local.ts && npm i",
    "start": "npm run format && ng serve",
    "format": "npm run format-glob -- \"src/{app,environments}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format-glob": "prettier --write",
    "ng": "ng",
    "build": "ng build",
    "stats": "ng build --prod --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "build:stats": "ng build --prod --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json",
    "prod": "http-server ./dist/browser -o -g",
    "build:prod": "ng build --target=production --environment=prod",
    "deploy": "ng build --prod --base-href /rog-map2/ && angular-cli-ghpages --no-silent --dir=dist/browser",
    "docs": "compodoc -p tsconfig.json",
    "docs:serve": "compodoc -p tsconfig.json -s",
    "docs:watch": "compodoc -p tsconfig.json -s -w",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-starter:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build-ie-polyfills": "webpack-cli src/polyfills-ie.ts -o src/assets/scripts/polyfills-ie.min.js -c webpack-polyfill.config.js"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "7.0.1",
    "@angular/animations": "7.0.0",
    "@angular/cdk": "7.0.2",
    "@angular/common": "7.0.0",
    "@angular/compiler": "7.0.0",
    "@angular/core": "7.0.0",
    "@angular/elements": "7.0.0",
    "@angular/forms": "7.0.0",
    "@angular/http": "7.0.0",
    "@angular/material": "7.0.1",
    "@angular/platform-browser": "7.0.0",
    "@angular/platform-browser-dynamic": "7.0.0",
    "@angular/platform-server": "7.0.0",
    "@angular/router": "7.0.0",
    "@angular/service-worker": "7.0.0",
    "@ng-bootstrap/ng-bootstrap": "3.3.1",
    "@ngrx/entity": "6.0.1",
    "@ngrx/store": "6.0.1",
    "@nguniversal/express-engine": "7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "7.0.0",
    "@schematics/update": "0.10.1",
    "@types/geojson": "^7946.0.4",
    "@types/mapbox-gl": "^0.51.3",
    "ag-grid-angular": "19.0.0",
    "ag-grid-community": "19.0.0",
    "ag-grid-enterprise": "19.0.0",
    "bootstrap": "4.1.3",
    "core-js": "2.5.7",
    "document-register-element": "1.11.0",
    "helpful-decorators": "1.7.2",
    "lodash": "4.17.11",
    "mapbox-gl": "^0.52.0",
    "ngx-auto-unsubscribe": "2.4.1",
    "ngx-contextmenu": "5.0.3",
    "node-sass": "4.9.4",
    "rxjs": "6.3.3",
    "ts-loader": "4.5.0",
    "typescript-fsa": "2.5.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.9.0-rc.2",
    "@angular/cli": "7.0.1",
    "@angular/compiler-cli": "7.0.0",
    "@angular/language-service": "7.0.0",
    "@compodoc/compodoc": "1.1.5",
    "@types/bingmaps": "0.0.1",
    "@types/express": "4.16.0",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/lodash": "4.14.88",
    "@types/node": "6.0.60",
    "angular-cli-ghpages": "0.5.3",
    "codelyzer": "4.4.4",
    "express": "4.16.3",
    "font-awesome": "4.7.0",
    "jasmine-core": "2.6.2",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "3.1.1",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "prettier": "1.16.0",
    "protractor": "5.4.1",
    "schematics": "file:schematics",
    "schematics-utilities": "^1.1.1",
    "ts-node": "3.2.0",
    "tslint": "5.11.0",
    "tslint-angular": "1.1.2",
    "typescript": "3.1.3",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.0"
  }
}
